colorscheme habamax
syntax on

set number

setg textwidth=999999999
set encoding=utf-8
set updatetime=300
set nocompatible
set completeopt+=noselect
set wildmenu
set wildmode=list:longest,list:full

set shiftwidth=4 smarttab
set expandtab
set tabstop=8 softtabstop=0
set showcmd

set splitright
set splitbelow
