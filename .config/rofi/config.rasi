configuration {
    modes: "window,drun,run,calc,emoji,ssh";
    show-icons: true;
    terminal: "kitty";
    ssh-client: "tmux new ssh";
    drun-show-actions: true;
    sidebar-mode: true;

    ssh {
        client: "tmux new ssh";
    }

    calc {
        calc-command: "echo -n '{result}' | wl-copy";
    }
}
